<?xml version="1.0" encoding="utf-8"?>
<executable>
  
  <category>Converter</category>
  <title>Vector Image to 4D Image Converter</title>
  <description>Convert a vector image to a 4D image.\n\
    Examples: \n\
    VectorTo4DImageConverter vectorImage.nii.gz 4dImage.nii.gz 
  </description>
  
  <documentation-url>http://www.unc.edu/~ptyap</documentation-url>
  
  <contributor>Pew-Thian Yap (ptyap@med.unc.edu)</contributor>
  <acknowledgements/>
  
  <license>http://www.unc.edu/~ptyap</license>
  
  <parameters>
    
    <image type="vector">
      <name>_InputFile</name>
      <label>Input File</label>
      <description>Input file.</description>
      <index>0</index>
      <channel>input</channel>
    </image>
    
    <image type="scalar">
      <name>_OutputFile</name>
      <label>Output File</label>
      <description>Output file.</description>
      <index>1</index>
      <channel>output</channel>
    </image>
    
  </parameters>
  
</executable>
